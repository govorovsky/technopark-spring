        </activity>
        <activity android:name=".SharedPreferencesActivity">
        </activity>
        <activity android:name=".DirectoriesActivity">
        </activity>
        <activity android:name=".filemanager.FileManagerActivity">
        </activity>
        <activity android:name=".db.SqliteActivity">
        </activity>
    </application>
</manifest>
